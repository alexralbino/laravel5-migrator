{
    "name": "alexralbino/laravel5-migrator",
    "description": "Improved version of Laravel 5's Migrator. Adds better error messaging and support for rollbacks with custom paths",
    "keywords": [
        "zingle",
        "laravel5-migrator",
        "laravel",
        "laravel5",
        "migrations",
        "rollbacks",
        "migrator"
    ],
    "homepage": "https://github.com/zingle/laravel5-migrator",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Jovin",
            "email": "njovin@zingle.me",
            "homepage": "http://www.zingle.me"
        }
    ],
    "require": {
        "illuminate/database": "^6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Zingle\\LaravelMigrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zingle\\LaravelMigrator\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
